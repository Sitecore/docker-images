<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <settings>
      <setting name="TenantId" value=""/>
      <!-- Config service basde URL -->
      <setting name="ML.Config.BaseUrl" value=""/>
      <!-- A token used to authenticate against config service for your tenantId -->
      <setting name="ML.Config.Token" value=""/>
    </settings>
    <pipelines>
      <initialize>
        <processor type="Sitecore.ML.Extension.Pipeline.Startup, Sitecore.ML.Extension"/>
      </initialize>
      <sessionEnd>
        <processor type="Sitecore.ML.Extension.Pipeline.SessionEnd, Sitecore.ML.Extension" />
      </sessionEnd>
      <renderField>
        <processor type="Sitecore.ML.Extension.Pipeline.AIRedirectOnLink, Sitecore.ML.Extension"/>
      </renderField>
      <convertToXConnectEvent>
        <processor patch:before="processor[@type='Sitecore.Analytics.XConnect.DataAccess.Pipelines.ConvertToXConnectEventPipeline.ConvertPageEventDataToEvent, Sitecore.Analytics.XConnect']" type="Sitecore.ML.Extension.Event.ConvertMLComponentViewedEvent, Sitecore.ML.Extension"/>
      </convertToXConnectEvent>
    </pipelines>
  </sitecore>
</configuration>