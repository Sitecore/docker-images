<?xml version="1.0" encoding="utf-8"?>
<Sitecore xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <Plugins>
    <Filters>
      <ContentHub xdt:Transform="Replace">+ContentHub</ContentHub>
    </Filters>
  </Plugins>
  <Host xdt:Transform="InsertIfMissing">
     <LicenseFilePath>C:/license/license.xml</LicenseFilePath>
  </Host>
  <FederatedUI>
    <HostBaseUrl xdt:Transform="Replace">https://sh.sc.local</HostBaseUrl>
  </FederatedUI>
</Sitecore>